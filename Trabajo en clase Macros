DIAPOSITIVA 1
%macro print_int 1
mov eax, 4
mov ebx, 1
mov ecx, %1
mov edx, 4
int 0x80
%endmacro
DIAPOSITIVA 2
section .data
array dd 1, 2, 3, 4, 5
DIAPOSITIVA 3
section .text
global _start
_start:
mov ecx, 0
mov eax, 0
DIAPOSITIVA 4
bucle:
mov ebx, [array + ecx*4]
add eax, ebx
inc ecx
cmp ecx, 5
jl bucle
DIAPOSITIVA 5
print_int eax
mov eax, 1
xor ebx, ebx
int 0x80
